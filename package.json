{
  "name": "geo4326",
  "version": "1.7.1",
  "description": "geo4326 converts footprint of satellite to polygon of EPSG:4326.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "lint": "yarn eslint ./src/",
    "build": "node script/generate.js && yarn tsc -p ./",
    "prepublishOnly": "yarn build"
  },
  "author": "yonda <yonda.fountain@gmail.com>",
  "homepage": "https://github.com/yonda-yonda/geo4326",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yonda-yonda/geo4326.git"
  },
  "keywords": [
    "typescript",
    "wgs84",
    "geojson",
    "vector",
    "gis",
    "geo",
    "geos"
  ],
  "packageManager": "yarn@3.8.1",
  "dependencies": {
    "@types/geojson": "^7946.0.14",
    "@types/proj4": "^2.5.5",
    "epsg-index": "^2.0.0",
    "geojson": "^0.5.0",
    "proj4": "^2.11.0",
    "satellite.js": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^29.5.12",
    "eslint": "^9.8.0",
    "eslint-plugin-jest": "^28.6.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.3",
    "tsc": "^2.0.4",
    "typescript": "^5.5.4",
    "typescript-eslint": "^7.17.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": [
        "ts-jest",
        {
          "tsconfig": "tsconfig.json"
        }
      ]
    },
    "testMatch": [
      "**/src/**/*.test.ts"
    ]
  }
}
